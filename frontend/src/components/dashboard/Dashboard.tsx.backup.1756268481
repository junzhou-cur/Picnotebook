'use client';

import React, { useState } from 'react';
import { Card, CardContent, Button } from '../ui';

interface Project {
  id: string;
  title: string;
  status: 'active' | 'completed' | 'in_progress';
  hypothesis: string;
  purpose: string;
  futureplan: string;
  notes: number;
  members: number;
  progress: number;
  updated: string;
}

